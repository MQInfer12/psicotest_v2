#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/mqinfer12/Code/psicotest_v2/_scripts/Batch API/node_modules/.pnpm/openai@4.91.0/node_modules/openai/bin/node_modules:/Users/mqinfer12/Code/psicotest_v2/_scripts/Batch API/node_modules/.pnpm/openai@4.91.0/node_modules/openai/node_modules:/Users/mqinfer12/Code/psicotest_v2/_scripts/Batch API/node_modules/.pnpm/openai@4.91.0/node_modules:/Users/mqinfer12/Code/psicotest_v2/_scripts/Batch API/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/mqinfer12/Code/psicotest_v2/_scripts/Batch API/node_modules/.pnpm/openai@4.91.0/node_modules/openai/bin/node_modules:/Users/mqinfer12/Code/psicotest_v2/_scripts/Batch API/node_modules/.pnpm/openai@4.91.0/node_modules/openai/node_modules:/Users/mqinfer12/Code/psicotest_v2/_scripts/Batch API/node_modules/.pnpm/openai@4.91.0/node_modules:/Users/mqinfer12/Code/psicotest_v2/_scripts/Batch API/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../openai/bin/cli" "$@"
else
  exec node  "$basedir/../openai/bin/cli" "$@"
fi
